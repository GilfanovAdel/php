<table border="1">
    <thead>
        <tr>
            <th>Пн</th>
            <th>Вт</th>
            <th>Ср</th>
            <th>Чт</th>
            <th>Пт</th>
            <th>Сб</th>
            <th>Вс</th>
        </tr>
    </thead>
    <tbody>
               {% for week in 1..5 %}
            <tr>
                {% for day in 1..7 %}
                    <td>{{ day + (week - 1) * 7 }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </tbody>
</table>
